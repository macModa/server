# Example environment variables for the app
# Copy this file to `.env` and fill in real values before running locally

# MongoDB Atlas connection string (replace placeholders)
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<your_db_name>?retryWrites=true&w=majority

# Server port (optional, defaults to 3000)
PORT=3000

# Example: add other secrets here (JWT_SECRET, etc.)
# JWT_SECRET=your_jwt_secret_here
